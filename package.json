{
  "name": "onecloset",
  "version": "1.0.0",
  "description": "แพลตฟอร์มเช่าเสื้อผ้าออนไลน์",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --turbo",
    "dev:verbose": "DEBUG=* next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules package-lock.json",
    "logs": "tail -f .next/server.log",
    "health": "curl -s http://localhost:3000/api/health || echo 'Server not responding'"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.5",
    "next-auth": "^4.24.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "prisma": "^6.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fashion",
    "rental",
    "ecommerce",
    "nextjs",
    "typescript"
  ],
  "author": "OneCloset Team",
  "license": "MIT"
}
